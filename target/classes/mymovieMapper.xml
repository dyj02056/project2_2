<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.movie.mymovie">
	<insert id="regMyMovie" parameterType="com.movie.mymovie.MyMovie">
		insert into mymovie(mm_id, mm_name) 
		values(#{mm_id}, #{mm_name})
	</insert>
</mapper>